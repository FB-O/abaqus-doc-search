
{
	"name": "Abaqus Doc Search",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".." 
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			],
			"settings": {
				"python.defaultInterpreterPath": "/usr/local/bin/python"
			}
		}
	},
	"mounts": [
		"source=${localWorkspaceFolder},target=/workspace,type=bind"
	],
	"workspaceFolder": "/workspace"
}
